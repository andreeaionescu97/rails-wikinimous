<h1>Articles</h1>

<ul>
  <% @articles.each do |article| %>
    <li><%= link_to article.title, article_path(article) %></li>
    <p><%= article.content %></p>
    <%= link_to "Delete an Article", article_path(article), method: :delete, data: { confirm: "Are you sure you want to delete?" } %>
  <% end %>
</ul>



<%= link_to "Add New Article", articles_new_path %>
